<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_folders">

    <fragment
        android:id="@+id/navigation_folders"
        android:name="com.example.app.ui.notes.NotesFragment"
        android:label="@string/folders"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_notes"
        android:name="com.example.app.ui.folders.FoldresFragment"
        android:label="@string/notes"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_add"
        android:name="com.example.app.ui.add.NotificationsFragment"
        android:label=""
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_todo"
        android:name="com.example.app.ui.todo.TodoFragment"
        android:label="fragment_todo"
        tools:layout="@layout/fragment_todo" />
    <fragment
        android:id="@+id/navigation_calendar"
        android:name="com.example.app.ui.calendar.CalendarFragment"
        android:label="fragment_calendar"
        tools:layout="@layout/fragment_calendar" />
</navigation>